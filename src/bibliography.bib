@article{Li2016a,
	doi = {10.1209/0295-5075/114/10007},
	url = {https://doi.org/10.1209/0295-5075/114/10007},
	year = 2016,
	month = {apr},
	publisher = {{IOP} Publishing},
	volume = {114},
	number = {1},
	pages = {10007},
	author = {Lei Li and Qing-Wen Wang and Shu-Qian Shen and Ming Li},
	title = "{Measurement-induced nonlocality based on Wigner-Yanase skew information}",
	journal = {{EPL} (Europhysics Letters)},
	abstract = {Measurement-induced nonlocality (MIN), which describes the maximum global effect caused by locally invariant measurements, was introduced by Luo and Fu (Phys. Rev. Lett., 106 (2011) 120401). In this paper, a new measure of MIN based on Wigner-Yanase skew information is proposed. It is shown that this measure not only has good computability but also eliminates the noncontractivity problem appearing in the original measure of MIN defined by the Hilbert-Schmidt norm. The analytical formulas of MIN based on Wigner-Yanase skew information for any pure states, -dimensional mixed states, and some higher-dimensional symmetric states are presented. Furthermore, the tight upper bound to MIN based on Wigner-Yanase skew information in the general case is also derived.}
}

@article{Sun2017,
	doi = {10.1209/0295-5075/118/60007},
	url = {https://doi.org/10.1209/0295-5075/118/60007},
	year = 2017,
	month = {jun},
	publisher = {{IOP} Publishing},
	volume = {118},
	number = {6},
	pages = {60007},
	author = {Yuan Sun and Yuanyuan Mao and Shunlong Luo},
	title = "{From quantum coherence to quantum correlations}",
	journal = {{EPL} (Europhysics Letters)},
	abstract = {In quantum mechanics, quantum coherence of a state relative to a quantum measurement can be identified with the quantumness that has to be destroyed by the measurement. In particular, quantum coherence of a bipartite state relative to a local quantum measurement encodes quantum correlations in the state. If one takes minimization with respect to the local measurements, then one is led to quantifiers which capture quantum correlations from the perspective of coherence. In this vein, quantum discord, which quantifies the minimal correlations that have to be destroyed by quantum measurements, can be identified as the minimal coherence, with the coherence measured by the relative entropy of coherence. To advocate and formulate this idea in a general context, we first review coherence relative to Lüders measurements which extends the notion of coherence relative to von Neumann measurements (or equivalently, orthonomal bases), and highlight the observation that quantum discord arises as minimal coherence through two prototypical examples. Then, we introduce some novel measures of quantum correlations in terms of coherence, illustrate them through examples, investigate their fundamental properties and implications, and indicate their applications to quantum metrology.}
}

