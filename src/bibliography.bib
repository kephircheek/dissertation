}


@Article{Lei2016,
author={Lei, Shuguo
and Tong, Peiqing},
title = "{Wigner--Yanase skew information and quantum phase transition in one-dimensional quantum spin-1/2 chains}",
journal={Quantum Information Processing},
year={2016},
month={Apr},
day={01},
volume={15},
number={4},
pages={1811-1825},
abstract={The quantum coherence based on Wigner--Yanase skew information and its relations with quantum phase transitions (QPTs) in one-dimensional quantum spin-1/2 chains are studied. Different from those at the critical point (CP) of the Ising transition in the transverse-field XY chain, the single-spin quantum coherence and the two-spin local {\$}{\$}{\backslash}sigma ^z{\$}{\$}quantum coherence are extremal at the CP of the anisotropy transition, and the first-order derivatives of the two-spin local {\$}{\$}{\backslash}sigma ^x{\$}{\$}and {\$}{\$}{\backslash}sigma ^y{\$}{\$}quantum coherence have logarithmic divergence with the chain size. For the QPT between the gapped and gapless phases in the chain with three-spin interactions, however, no finite-size scaling behavior of the derivatives of quantum coherence is found.},
issn={1573-1332},
doi={10.1007/s11128-016-1244-9},
url={https://doi.org/10.1007/s11128-016-1244-9}
}

@article{Qiu2017,
title = "{Skew information in the XY model with staggered Dzyaloshinskii-Moriya interaction}",
journal = {Physica B: Condensed Matter},
volume = {514},
pages = {13-18},
year = {2017},
issn = {0921-4526},
doi = {https://doi.org/10.1016/j.physb.2017.03.018},
url = {https://www.sciencedirect.com/science/article/pii/S0921452617301278},
author = {Liang Qiu and Dongxiao Quan and Fei Pan and Zhi Liu},
keywords = {Skew information, The XY spin chain, Staggered Dzyaloshinskii-Moriya interaction, Quantum phase transitions},
abstract = {We study the performance of the lower bound of skew information in the vicinity of transition point for the anisotropic spin-1/2 XY chain with staggered Dzyaloshinskii-Moriya interaction by use of quantum renormalization-group method. For a fixed value of the Dzyaloshinskii-Moriya interaction, there are two saturated values for the lower bound of skew information corresponding to the spin-fluid and Néel phases, respectively. The scaling exponent of the lower bound of skew information closely relates to the correlation length of the model and the Dzyaloshinskii-Moriya interaction shifts the factorization point. Our results show that the lower bound of skew information can be a good candidate to detect the critical point of XY spin chain with staggered Dzyaloshinskii-Moriya interaction.}
}

